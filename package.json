{
  "name": "ldschur.ch",
  "version": "1.0.0",
  "description": "A URL shortening service focused on sharing links from or about The Church of Jesus Christ of Ladder Day Saints",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infiniteoo/LDSchur.ch.git"
  },
  "keywords": [
    "url",
    "shortening",
    "service",
    "LDS",
    "church",
    "of",
    "jesus",
    "christ",
    "of",
    "ladder",
    "day",
    "saints"
  ],
  "author": "troy dorman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infiniteoo/LDSchur.ch/issues"
  },
  "homepage": "https://github.com/infiniteoo/LDSchur.ch#readme",
  "dependencies": {
    "concurrently": "^6.2.1",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "emotion": "^11.0.0",
    "express": "^4.17.1",
    "is-porn": "^0.9.0",
    "mongoose": "^5.13.6",
    "psl": "^1.8.0",
    "shortid": "^2.2.16",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
